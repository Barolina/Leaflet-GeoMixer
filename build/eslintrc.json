{
  "rules": {
    "camelcase": 2,
    "quotes": [2, "single"],
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "space-before-function-paren": 0,
    "object-curly-spacing": ["error", "never"],
    "keyword-spacing": ["error"],
    "no-lonely-if": 2,
    "comma-style": 2,
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "strict": 0,
    "key-spacing": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-unused-expressions": ["error", { "allowShortCircuit": true, "allowTernary": true }]
  },
  "globals": {
    "L": true,
    "module": false,
    "require": false,
    "define": false,
    "ActiveXObject": true,
    "Deferred": true,
    "gmxImageLoader": true,
    "gmxParsers": true,
    "gmxSessionManager": true,
    "DataManager": true,
    "StyleManager": true,
    "ScreenVectorTile": true,
    "gmxImageTransform": true,
    "ProjectiveImage": true,
    "VectorTile": true,
    "Observer": true,
    "gmxVectorTileLoader": true,
    "TilesTree": true,
    "gmxMapManager": true,
    "gmxMap": true,
    "gmxAPIutils": true
  },
  "env": {
    "browser": true
  }
}
